{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev ./src/index",
    "lint": "eslint --ext .ts,.tsx --fix src/",
    "prettify": "prettier --write src/",
    "fmt-lint": "yarn run prettify && yarn run lint",
    "start": "node ./dist/src/index.js",
    "seed": "npx prisma migrate reset && ts-node prisma/seed.ts",
    "build": "npx prisma generate && tsc",
    "deploy": "npm run build && npx prisma migrate deploy && node ./dist/src/index.js",
    "deploytests": "npm run build && dotenv -e .env.test node ./dist/src/index.js",
    "test": "dotenv -e .env.test jest --watch && dotenv -e .env.test -- npx prisma migrate reset -f",
    "migrate:postgres": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init && dotenv -e .env -- npx prisma migrate dev --name postgres-init",
    "reset:postgres": "dotenv -e .env.test -- npx prisma migrate reset -f && dotenv -e .env -- npx prisma migrate reset -f",
    "reset:tests": "dotenv -e .env.test  npx prisma migrate reset -f",
    "studio:test": "dotenv -e .env.test -- npx prisma studio"
  },
  "_moduleDirectories": [
    "src"
  ],
  "dependencies": {
    "@prisma/client": "3.11.0",
    "apollo-server": "^3.5.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.0",
    "dropbox": "^10.22.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.4.0",
    "faker": "^5.5.3",
    "file-metadata": "^3.0.0",
    "graphql": "15.7.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.13.6",
    "graphql-upload": "^12.0.0",
    "graphql-ws": "^5.8.1",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.1.1",
    "minio": "^7.0.27",
    "nodemailer": "^6.7.5",
    "prisma": "3.11.0",
    "reflect-metadata": "^0.1.13",
    "stream-promise": "^3.2.0",
    "ts-jest": "^27.0.7",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.19.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/graphql-fields": "^1.3.4",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/luxon": "^2.0.7",
    "@types/minio": "^7.0.13",
    "@types/node": "^16.11.6",
    "@types/nodemailer": "^6.4.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.7",
    "jest": "^27.3.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.3.2",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.4.4",
    "ws": "^8.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
